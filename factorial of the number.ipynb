    "#factorial of number\n",
    "num=int(input(\"enter a number\"))\n",
    "i=1\n",
    "fact=1\n",
    "while(i<=num):\n",
    "    fact=fact*i\n",
    "    i=i+1\n",
    "print(\"the factorial is\",fact)"
